<table class="table">
        <tbody>
          <% @favorite_posts.each do |post| %>
          <tr>
            <td><%= image_tag post.get_image(150,150) %></td>
            <td>
              <%= link_to post.event_name, post_path(post.id) %>
            </td>
          </tr>
          <% end %>
        </tbody>
      </table>